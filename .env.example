# Copy this to .env and fill in your actual values

# Required: Your Gemini API key from https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Your Kie.ai API key for image generation
KIE_API_KEY=your_kie_api_key_here

# Optional: Your DeepSeek API key for translation
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Optional: Port for the application
PORT=8000

# Optional: Host binding (0.0.0.0 for all interfaces)
HOST=0.0.0.0
